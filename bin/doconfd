# $Header$

if [ ${#} -lt 1 ] ; then
	echo "doconfd: at least one argument needed"
	exit 1 	
fi

tmp_INSDESTTREE="${INSDESTTREE}"
INSDESTTREE=/etc/conf.d/
doins "$@"
INSDESTTREE="${tmp_INSDESTTREE}"
unset tmp_INSDESTTREE
