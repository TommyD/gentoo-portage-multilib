# $Header$

for x in "${D}"opt/*/man "${D}"usr/share/man "${D}"usr/local/man "${D}"usr/X11R6/man ; do
	if [ -d "${x}" ] ; then
		prepman "`echo "${x}" | sed -e "s:${D}::" -e "s:/man[/]*$::"`"
		export prepallman_banner=no
	fi
done
