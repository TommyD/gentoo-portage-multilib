import sys, os
if len(sys.argv)!=3:
	print "xpak: expecting three arguments."
	sys.exit(1)

try:
	import xpak
except ImportError:
	sys.path.insert(0, "/usr/lib/portage/pym")
	import xpak

xpak.xpak(sys.argv[1],sys.argv[2])
