import sys
if len(sys.argv)!=3:
	print "xpak: expecting three arguments."
	sys.exit(1)

sys.path = ["/usr/lib/portage/pym"]+sys.path

import xpak
xpak.xpak(sys.argv[1],sys.argv[2])
