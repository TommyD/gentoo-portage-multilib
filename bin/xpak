import sys, os
if len(sys.argv)!=3:
	print "xpak: expecting three arguments."
	sys.exit(1)

try:
	import xpak
except ImportError:
	from os import path as osp
	sys.path.insert(0, osp.join(osp.dirname(osp.dirname(osp.realpath(__file__))), "pym"))
	import xpak

xpak.xpak(sys.argv[1],sys.argv[2])
