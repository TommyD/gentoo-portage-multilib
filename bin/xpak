import sys, os
if len(sys.argv)!=3:
	print "xpak: expecting three arguments."
	sys.exit(1)

sys.path.insert(0, os.environ.get("PORTAGE_PYM_PATH", "/usr/lib/portage/pym"))

import xpak
xpak.xpak(sys.argv[1],sys.argv[2])
