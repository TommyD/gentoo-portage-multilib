# $Id$

if [[ -z ${T} ]] || [[ -z ${2} ]] ; then
	echo "$0: Need two arguments, old file and new file" 1>&2
	exit 1
fi

rm -rf "${T}/${2}" && \
cp -f "${1}" "${T}/${2}" && \
exec doexe "${T}/${2}"
