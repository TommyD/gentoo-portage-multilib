if [ ${#} -lt 1 ] ; then
	echo "doenvd: at least one argument needed"
	exit 1
fi

tmp_INSDESTTREE="${INSDESTTREE}"
INSDESTTREE=/etc/env.d/
doins "$@"
INSDESTTREE="${tmp_INSDESTTREE}"
unset tmp_INSDESTTREE
