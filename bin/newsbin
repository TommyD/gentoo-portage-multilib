# $Id: /var/cvsroot/gentoo-src/portage/bin/newsbin,v 1.7 2004/10/04 13:56:50 vapier Exp $

if [ -z "${T}" ] || [ -z "${2}" ] ; then
	echo "Nothing defined to do."
	exit 1
fi

rm -rf "${T}/${2}"
cp "${1}" "${T}/${2}"
dosbin "${T}/${2}"
